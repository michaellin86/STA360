b.samp[1] = b
n.samp = rep(NA,N)
n.samp[1] = n
for(i in 2:N){
b.samp[i] = rbeta(1, y+1, n.samp[i-1]-y+1)
n.samp[i] = y + rpois(1, (1-b.samp[i])*lam)
}
